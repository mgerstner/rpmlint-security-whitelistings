{
	"bind-chrootenv": {
		"audits": {
			"bsc#1174642": {
				"comment": "Chroot duplicate of the log socket. Packaged as %ghost, therefore 'appears' as a regular file.",
				"meta": {
					"/var/lib/named/dev/log": {
						"type": "-",
						"mode": "0666",
                                                "owner": "root:root"
					}
				}
			}
		}
	},
